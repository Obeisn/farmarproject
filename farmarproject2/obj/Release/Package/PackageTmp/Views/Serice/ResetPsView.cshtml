@model farmarproject2.Models.AspNetUser
@{
    Layout = null;
}

<form id="send" enctype="multipart/form-data">
    @Html.AntiForgeryToken()
    <div class="form-group">
        @Html.LabelFor(model => model.Email, new { @class = "control-label" })
        @Html.EditorFor(model => model.Email, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
        @Html.ValidationMessageFor(model => model.Email)
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.FamName, new { @class = "control-label" })
        @Html.EditorFor(model => model.FamName, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(model => model.FamName)
    </div>
    <div class="form-group">
        @Html.HiddenFor(model => model.Id, new { @class = "control-label" })
        @Html.HiddenFor(model => model.UserName, new { @class = "control-label" })
        @Html.HiddenFor(model => model.PasswordHash, new { @class = "control-label" })
    </div>
    <div class="form-group">
        <button style="margin-left:90px; margin-top:10px;" onclick="return ResetPs();" class="btn btn-danger">
            重設密碼
        </button>
    </div>
</form>


